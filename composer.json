{
    "name": "antoniosiles/nova-4-card-map-plus",
    "description": "A Laravel Nova card plus.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "AntonioSiles\\Nova4CardMap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioSiles\\Nova4CardMap\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
